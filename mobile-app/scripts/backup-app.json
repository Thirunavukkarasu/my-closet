{
  "expo": {
    "name": "my-closet",
    "slug": "my-closet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thirunavukkarasu.mycloset"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thirunavukkarasu.mycloset"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos."
        }
      ],
      [
        "expo-splash-screen",
        {
          "autoHide": true,
          "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
          }
        }
      ],
      [
        "expo-status-bar",
        {
          "style": "light",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-secure-store",
        {
          "isAccessSecureStoreEnabled": true
        }
      ],
      [
        "expo-constants",
        {
          "isAccessConstantsEnabled": true
        }
      ],
      [
        "expo-file-system",
        {
          "isAccessFileSystemEnabled": true
        }
      ],
      [
        "expo-permissions",
        {
          "isAccessPermissionsEnabled": true
        }
      ],
      [
        "expo-notifications",
        {
          "isAccessNotificationsEnabled": true
        }
      ],
      [
        "expo-contacts",
        {
          "isAccessContactsEnabled": true
        }
      ],
      [
        "expo-calendar",
        {
          "isAccessCalendarEnabled": true
        }
      ],
      [
        "expo-location",
        {
          "isAccessLocationEnabled": true
        }
      ],
      [
        "expo-sensors",
        {
          "isAccessSensorsEnabled": true
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "isAccessBarcodeScannerEnabled": true
        }
      ],
      [
        "expo-av",
        {
          "isAccessAVEnabled": true
        }
      ],
      [
        "expo-font",
        {
          "isAccessFontEnabled": true
        }
      ],
      [
        "expo-secure-store",
        {
          "isAccessSecureStoreEnabled": true
        }
      ],
      [
        "expo-sqlite",
        {
          "isAccessSQLiteEnabled": true
        }
      ],
      [
        "expo-branch",
        {
          "isAccessBranchEnabled": true
        }
      ],
      [
        "expo-facebook",
        {
          "isAccessFacebookEnabled": true
        }
      ],
      [
        "expo-google-sign-in",
        {
          "isAccessGoogleSignInEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8cde9049-5e24-4efe-9640-e5f9fbd011da"
      }
    },
    "owner": "thirunavukkarasu"
  }
}
